{
  "version": 2,
  "functions": {
    "apichat.py": {
      "runtime": "@vercel/python"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/api/chat",
      "dest": "/apichat.py"
    },
    {
      "src": "/api/health", 
      "dest": "/apichat.py"
    },
    {
      "src": "/(.*)",
      "dest": "/apichat.py"
    }
  ]
}